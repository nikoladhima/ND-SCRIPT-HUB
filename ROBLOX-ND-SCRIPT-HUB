local Window = Fluent:CreateWindow({
    Title = "ND ScriptHubâ€Ž " ..Fluent.Version,
    SubTitle = "by Nikoleto Scripts",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely (false recommended)
    Theme = "Gremple",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Game_Scripts = Window:AddTab({ Title = "Game Scripts", Icon = "" }),
    Admin_Scripts = Window:AddTab({ Title = "Admin Scripts", Icon = "" }),
    Universal_Scripts = Window:AddTab({ Title = "Universal Scripts", Icon = ""}),
    Anti_Virus = Window:AddTab({ Title = "Anti Virus", Icon = "" }),
    Extras = Window:AddTab({ Title = "Extras", icon = ""}),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

