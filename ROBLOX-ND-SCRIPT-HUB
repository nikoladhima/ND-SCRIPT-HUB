local Fluent = loadstring(game:HttpGet("https://raw.githubusercontent.com/nikoladhima/Fluent/refs/heads/master/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/nikoladhima/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/nikoladhima/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "ND ScriptHub"    .. Fluent.Version,
    SubTitle = "by nikoleto bonnie",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = false, -- The blur may be detectable, setting this to false disables blur entirely (false recommended)
    Theme = "Gremple",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Game_Scripts = Window:AddTab({ Title = "Game Scripts", Icon = "" }),
    Admin_Scripts = Window:AddTab({ Title = "Admin Scripts", Icon = "" }),
    Universal_Scripts = Window:AddTab({ Title = "Universal Scripts", Icon = ""}),
    Anti_Virus = Window:AddTab({ Title = "Anti Virus", Icon = "" }),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

do
    Fluent:Notify({
        Title = "IMPORTANT NOTE",
        Content = "Please use the Anti-Virus before executing any scripts to prevent scripts from stealing your info",
        SubContent = "e", -- Optional
        Duration = 5 -- Set to nil to make the notification not disappear
    })



    Tabs.Game_Scripts:AddParagraph({
        Title = "Game scripts (still in development)",
        Content = "This is a paragraph.\nSecond line!"
    })



    Tabs.Game_Scripts:AddButton({
        Title = "Breaking Plus+",
        Name = "BP+",
        Description = "The best Breaking Point script",
        Callback = function()
            Window:Dialog({
                Title = "BP+",
                Content = "Breaking Point",
                Buttons = {
                    {
                        Title = "Load script",
                        Callback = function()
                            loadstring(game:HttpGet("https://raw.githubusercontent.com/NaikoScript/Breaking-Plus/main/Script"))()
                            print("Loaded Breaking Plus+")
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled")
                        end
                    }
                }
            })
        end
    })
Tabs.Game_Scripts:AddButton({
